    function openNewTab() {

      const params = new URLSearchParams(window.location.search);

      const conversationId = params.get('conversationId'); // Example: ?name=John

      

      window.open(`https://digital-uat.mashreq.com/faceapp?conversationId=${conversationId}`, '_blank');

    }

